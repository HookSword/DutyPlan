@using DutyPlan.Common.DB
<div class="col-xs-3" style="margin-left:2px">
    <table class="tableDms">
        <tbody>
            <tr>
                <th class="thspecial">Product</th>
                <th>TR25</th>
                <th>TR26</th>
                <th>TR27</th>
                <th>TR28</th>
                <th>TR30</th>
                <th>TR32</th>
                <th>TR33</th>
