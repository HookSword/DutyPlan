                <td rowspan="2"><p>MI</p>fixed</td>
                @for (var i = 8; i < 11; i++)
                {
                    var id = "st" + i;
                    <td rowspan="2" class="tdimg"><img id="@id" src="@OperationDb.GetlocationById(id)" /></td>
                }
                <td rowspan="2" class="tdimg">
                    <p>PPL</p>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Random&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
