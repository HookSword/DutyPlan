@using DutyPlan.Common.DB
<div class="col-xs-2">
    <table class="tableDms">
        <tbody>
            <tr>
                <th class="thspecial" colspan="2">ALC</th>
                <th colspan="2">Haloriel</th>
            </tr>
            <tr>
                @for (var i = 1; i < 5; i++)
                {
                    var id = "dm" + i;
                    <td rowspan="2" class="tdimg"><img id="@id" src="@OperationDb.GetlocationById(id)" /></td>
                }
            </tr>
            <tr></tr>
            <tr>
                <th colspan="4" class="thspecial">GC/FG</th>
            </tr>
            <tr>
                @for (var i = 5; i < 9; i++)
                {
                    var id = "dm" + i;
                    <td rowspan="2" class="tdimg"><img id="@id" src="@OperationDb.GetlocationById(id)" /></td>
                }
            </tr>
            <tr></tr>
        </tbody>
    </table>
</div>
