@using DutyPlan.Common.DB
<div class="col-xs-2 slipring">
    <table class="tableDms">
        <tbody>
            <tr>
                <th class="thspecial" style="width:103.73px!important">Product</th>
                <th>W1</th>
                <th>W2</th>
                <th></th>
            </tr>
            <tr>
                <td rowspan="2"><p>PPL</p></td>
                @for (var i = 1; i < 4; i++)
                {
                    var id = "sas" + i;
                    <td rowspan="2" class="tdimg"><img id="@id" src="@OperationDb.GetlocationById(id)" /></td>
                }
            </tr>
            <tr></tr>
        </tbody>
    </table>
</div>
