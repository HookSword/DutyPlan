@using DutyPlan.Common.DB
<div class="col-xs-3" style="margin-left:2px">
    <table class="tableDms">
        <tbody>
            <tr>
                <th class="thspecial" colspan="4"></th>
                <th colspan="2"></th>
                <th colspan="2"></th>
            </tr>
            <tr>
                @for (var i = 1; i < 9; i++)
                {
                    var id = "oo" + i;
                    <td rowspan="2" class="tdimg"><img id="@id" src="@OperationDb.GetlocationById(id)" /></td>
                }
            </tr>
            <tr></tr>
            <tr>
                <th colspan="6" class="thspecial"></th>
                <th colspan="2"></th>
            </tr>
            <tr>
                @for (var i = 9; i < 17; i++)
                {
                    var id = "oo" + i;
                    <td rowspan="2" class="tdimg"><img id="@id" src="@OperationDb.GetlocationById(id)" /></td>
                }
            </tr>
            <tr></tr>
        </tbody>
    </table>
</div>
