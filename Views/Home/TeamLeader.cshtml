@using DutyPlan.Common.DB
<div class="col-xs-2 teamleader" style="margin-left:3px">
    <table class="tableDms">
        <tbody>
            <tr>
                <th class="thspecial">TCO</th>
                <th>DMS</th>
                <th>Asm</th>
                <th>Test</th>
                <th>F1</th>
            </tr>
            <tr>
                @for (var i = 1; i <6; i++)
                {
                    var id = "tl" + i;
                    <td rowspan="2" class="tdimg"><img id="@id" src="@OperationDb.GetlocationById(id)" /></td>
                }
            </tr>
            <tr></tr>
        </tbody>
    </table>
</div>

